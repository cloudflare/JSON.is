@import nib

@import "ui/styl/reset"
@import "ui/styl/box-sizing-border-box-all"
@import "ui/styl/body-typography"
@import "ui/styl/responsive-helpers"
@import "ui/styl/subtle-text"
@import "ui/styl/hr"
@import "ui/styl/h1-h6"
@import "ui/styl/links-brand-colored"
@import "ui/styl/icons"
@import "ui/styl/more-links-and-buttons"
@import "ui/styl/loading-dots"
@import "ui/styl/box"
@import "ui/styl/colors"
@import "ui/styl/code-colors"
@import "ui/styl/fonts"
@import "ui/styl/logo"
@import "ui/styl/font-smoothing"

navigationWidth = 15rem
smallNavigationWidth = 13rem

codeOverlayWidth = 25rem
minimumEditorWidth = 25rem
editorWithCodeOverlayMinWidthQuery = "(min-width: " + (navigationWidth + codeOverlayWidth + minimumEditorWidth) + ")"
editorWithCodeOverlayMaxWidthQuery = "(max-width: " + (navigationWidth + codeOverlayWidth + minimumEditorWidth - .0625em) + ")"

codeOverlayWidthLarge = 30rem
minimumEditorWidthLarge = 45rem
editorWithCodeOverlayLargeMinWidthQuery = "(min-width: " + (navigationWidth + codeOverlayWidthLarge + minimumEditorWidthLarge) + ")"
editorWithCodeOverlayLargeMaxWidthQuery = "(max-width: " + (navigationWidth + codeOverlayWidthLarge + minimumEditorWidthLarge - .0625em) + ")"

mobileMessageMaxWidthQuery = "(max-width: " + (smallNavigationWidth + minimumEditorWidth + 5em) + ")"

::-moz-selection
  background #e0dae0
  text-shadow none

::selection
  background #e0dae0
  text-shadow none

html
  antialiasedFonts()
  font-size 16px
  background #eee

  &[data-format]
    overflow hidden

.page-loader
  position fixed
  top 0
  right 0
  bottom 0
  left navigationWidth
  background #eee
  z-index 999
  transition opacity .15s ease
  text-align center
  line-height 100vh

  @media editorWithCodeOverlayMaxWidthQuery
    left smallNavigationWidth

  .page-loader-inner
    width 20em
    height 2em
    max-width 100%
    margin auto
    text-align center

  .loading-dots
    margin-left -1.5em

  html.page-loaded &
    pointer-events none
    opacity 0

    *
      animation-play-state paused

.navigation
  position fixed
  top 0
  left 0
  bottom 0
  width navigationWidth
  padding-top 2.8em
  background-color #111
  background-image linear-gradient(#111, #1f0514)
  z-index 5

  @media editorWithCodeOverlayMaxWidthQuery
    width smallNavigationWidth

  .mobile-message
    display none

  @media mobileMessageMaxWidthQuery
    width 100%
    min-height 100vh
    position relative

    .mobile-message
      display block
      color rgba(#fff, .8)
      padding 0 2em 4em

ul.side-navigation-list
  list-style none
  padding 0
  margin 0
  line-height 1.5em
  font-size 1.1875em

  @media mobileMessageMaxWidthQuery
    display none

  > li

    &.separator
      height .0625em
      margin .375em 0

      &.large
        height 2.5em

    > a
      display block
      position relative
      cursor pointer
      padding 0.45em 2rem .45em 2rem

      &, &:hover
        color rgba(#fff, .8)
        text-decoration none

      &:hover
        background rgba(#fff, .1)

      &.active

        &, &:hover
          background #fff
          color #000

.share-target, a.attribution
  position absolute
  cursor pointer
  bottom 0
  height 60px
  background #fff
  text-align center
  border-right 1px solid #dfd1d9

  &:hover, &.share-enabled
    background #efe6eb

.share-target
  left 0
  right 50%
  line-height 60px
  font-size 1.1875em

  &, &:hover
    color #1f0514

  span
    text-transform uppercase
    font-size .7em
    letter-spacing .07em
    text-indent @letter-spacing

.share.share-element
  width navigationWidth

  @media editorWithCodeOverlayMaxWidthQuery
    width smallNavigationWidth

a.attribution
  left 50%
  right 0
  padding 1.25em

  &:not(:hover) path
    fill #1f0514

  .logo-link
    position relative
    font-size .23em
    vertical-align middle

  @media mobileMessageMaxWidthQuery
    border-right 0

.json-is-logo
  text-decoration none
  font-size 2em
  letter-spacing -.04em
  font-weight bold
  display inline-block
  line-height .8

  &, &:hover
    color #fff

  .navigation &
    margin-bottom 1em
    margin-left 2rem

.page
  position relative
  padding-left navigationWidth
  background #eee

  @media editorWithCodeOverlayMaxWidthQuery
    padding-left smallNavigationWidth

  @media mobileMessageMaxWidthQuery
    display none

.code-editor
  position relative
  overflow hidden
  width 100%
  height 100%
  z-index 10

  #editor
    font-size 16px // Note: this number is crucial to the JS
    line-height 21px // Note: this number is crucial to the JS
    height 100%
    height 100vh
    width 100%

  .code-property-path
    position fixed
    z-index 4
    bottom 0
    right 15px // later set by scrollbar width calculated with JS
    background #fff
    padding 5px
    color subtle-text
    font-size .8em

    &:empty
      display none !important

  .code-overlay
    position fixed
    z-index 5
    top 0
    right 0
    width 50%
    width codeOverlayWidth
    max-height 100vh
    overflow-y scroll
    -webkit-overflow-scrolling touch
    line-height 1.3em
    background #fff
    opacity 0
    pointer-events none

    @media editorWithCodeOverlayLargeMinWidthQuery
      width codeOverlayWidthLarge

    &[target-off-screen]
      display none !important

    &[overflow-y="true"]

      .code-overlay-content
        animation bounceInUp .8s

      &:hover:after
        opacity 0

    @media editorWithCodeOverlayMinWidthQuery
      opacity 1
      pointer-events all

    h4, h5
      font-weight bold

    h4
      font-size 1.2em
      margin-bottom 0.8333em

    h5
      font-size 1em

    pre
      line-height 1.18em
      font-size .85em
      background-color #eee
      padding 1em
      color #222
      white-space pre
      word-wrap normal
      overflow-x auto

    .code-overlay-content
      transform translate3d(0, 0, 0)
      padding 21px

      &:empty
        display none

      > *:first-child
        margin-top 0

      > *:last-child
        margin-bottom 0

      *:not(pre) > code
        font-family monospaceFonts
        font-size 0.727272em
        display inline-block
        vertical-align middle
        margin 0 .0625em
        position relative
        top -1px
        background-color #eee
        padding 0 .45em

@keyframes bounceInUp
  0%, 60%, 75%, 90%, 100%
    animation-timing-function cubic-bezier(.215, .610, .355, 1)

  0%
    opacity 0
    transform translate3d(0, 150px, 0)

  60%
    opacity 1
    transform translate3d(0, -20px, 0)

  75%
    transform translate3d(0, 10px, 0)

  90%
    transform translate3d(0, -5px, 0)

  100%
    transform translate3d(0, 0, 0)

// Ace editor styles

html .ace-tm
  color #000

  &, .ace_gutter
    background #eee

  .ace_gutter
    color #a5a5a5

    .ace_fold-widget
      transition opacity .2s ease

    &:not(:hover) .ace_fold-widget
      opacity .5

  .ace_gutter-active-line, .ace_marker-layer .ace_active-line
    background #fff

  .ace_cursor
    color #000

  .ace_marker-layer

    .ace_selection
      background #e0dae0

    .ace_step
      background rgb(102, 82, 0)

    .ace_bracket
      margin -1px 0 0 -1px
      border 1px solid #000

    .ace_selected-word
      background #fafaff
      border 1px solid #000

  .ace_fold
    background-color darken(codePink, 20%)
    border-color rgba(#000, .4)

  .ace_string
    color codeBlue

  .ace_indent-guide
    background url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAACCAYAAACZgbYnAAAAEklEQVQImWPQ0FD0ZXBzd/wPAAjVAoxeSgNeAAAAAElFTkSuQmCC") right repeat-y
    opacity .2

  .ace_constant.ace_numeric
    color #5100db
    color codeBlue

  .ace_entity.ace_name.ace_function, .ace_entity.ace_other, .ace_entity.ace_other.ace_attribute-name, .ace_variable
    color codePink

// Home page styles

html.json-is-home-page
  min-height 100vh
  background-color #111
  background-image linear-gradient(#111, #1f0514)
  color rgba(#fff, .65)
  text-align center

  body
    padding 5em 1em
    min-height 100vh
    display flex
    align-items center
    justify-content center

    @media (max-width 65em)
      display block

  header a
    color #fff

  ul.docs
    padding-left 3em
    padding-right 3em
    max-width 80em
    margin-left auto
    margin-right auto
    display block
    list-style none
    padding 0
    perspective 1000px

    @media (min-width 110em)
      max-width 100em

    > *:hover
      transform translate3d(0, 0, -1.2rem)

    @media (max-width 65em)
      display block
      max-width 100%

    > li
      flex 1
      padding 1.5em
      transition transform .3s
      display inline-block
      perspective 100rem
      width 25em
      max-width 100%

      @media (min-width 110em)
        width 30em

      @media (max-width 65em)
        display block
        margin-left auto
        margin-right auto
        width 80%

      @media (max-width 40em)
        width 100%

      > a
        display block
        height 20em
        color #fff
        user-select none
        transition color .13s ease, background-color .13s ease, transform .3s
        perspective 100rem

        &:hover
          color #000

          > div
            background #fff

        > div
          display block
          height 20em
          overflow hidden
          border-radius 3px
          padding-top 2em
          background rgba(#fff, .06)
          transition background-color .13s ease

          > button.more.title
            position relative
            background none
            border 0
            padding 0
            font-size 2em
            color inherit
            letter-spacing -0.025em

          > pre
            padding 1em
            text-align left
            margin 0
            white-space pre
            word-wrap normal
            overflow hidden
            font-size .8em
            color #61324b

  footer
    color rgba(#fff, .6)

    p.created
      font-size 1.2em

    a
      color #fff

// Tour

.tour
  position fixed
  z-index 20
  top 0
  right 0
  bottom 0
  left 0
  background linear-gradient(rgba(#111, .95), rgba(#1f0514, .95))
  text-shadow 0 1px #000
  cursor pointer
  opacity 0
  animation tour-fade-in .5s ease forwards
  overflow hidden
  display none

  @media editorWithCodeOverlayMinWidthQuery
    display block

  .tour-content
    position absolute
    left navigationWidth
    padding 4em
    width 37.5em
    max-width 100%
    opacity 0
    transform translate3d(0, -10em, 0)
    animation tour-content-fade-in .5s .5s ease forwards

    @media editorWithCodeOverlayMaxWidthQuery
      left smallNavigationWidth

  &.done
    opacity 1
    animation tour-fade-out .5s .5s cubic-bezier(.75, 0, .75, .9) forwards
    pointer-events none

    .tour-content
      opacity 1
      transform translate3d(0, 0, 0)
      animation tour-content-fade-out .8s cubic-bezier(.75, 0, .75, .9) forwards

  h1
    margin 0 0 1.25rem
    font-size 2em
    line-height 1em
    font-weight 300
    color #865d75

  p
    margin 0 0 1.5rem
    font-size 1.1em
    line-height 1.42em
    font-weight 300
    color #fff

    &.abutting
      margin-bottom .5rem

    em
      margin-left .125em
      margin-right .125em

    code
      font-family monospaceFonts
      font-size 0.8em
      display inline-block
      vertical-align middle
      margin 0 .0625em
      position relative
      top -1px
      background rgba(#fff, .05)
      padding 0 .45em

  button
    cursor pointer
    border 0
    font-size 1.5em
    font-family inherit
    padding .5em 1.1em
    background rgba(#fff, .08)
    color #fff
    border-radius .1875rem

    &:focus
      outline none

    &:hover
      background rgba(#fff, .15)

  &:active button, button:active
    background rgba(#fff, .025)
    color rgba(#fff, .6)

@keyframes tour-fade-in
  to
    opacity 1

@keyframes tour-content-fade-in
  to
    opacity 1
    transform translate3d(0, 0, 0)

@keyframes tour-fade-out
  to
    opacity 0

@keyframes tour-content-fade-out
  50%
    opacity 0
    transform translate3d(0, 6em, 0)
  to
    opacity 0
    transform translate3d(0, 12em, 0)

// Share

@css {

.share, .share:after, .share:before, .share *, .share *:after, .share *:before {
  -webkit-user-select: none;
  -moz-user-select: none;
  user-select: none
}

.share-element {
  position: absolute;
  display: none
}

  .share-element.share-open {
    display: block
  }

@font-face {
  font-family: "share-icons";
  font-style: normal;
  font-weight: normal;
  src: url(data:application/x-font-woff;charset=utf-8;base64,) format("woff")
}

.share [class^="share-icon-"]:before {
  font-family: "share-icons";
  font-style: normal;
  font-weight: normal
}

.share-icon-share:before {
  content: "\ea7d"
}

.share-icon-email:before {
  content: "\e945"
}

.share-icon-twitter:before {
  content: "\ea91"
}

.share-icon-facebook:before {
  content: "\ea8d"
}

.share-icon-github:before {
  content: "\eab1"
}

.share.share-element {
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
  -webkit-transition: opacity 500ms;
  transition: opacity 500ms;
  opacity: 0.999;
  z-index: 10;
}

  .share.share-element.share-open {
    display: none
  }

  .share.share-element.share-open-transitionend {
    display: block
  }

  .share.share-element.share-after-open {
    -webkit-transition: none;
    transition: none;
    opacity: 1
  }

  .share.share-element .share-content {
    overflow: hidden;
    position: relative
  }

    .share.share-element a[class^="share-icon-"], .share.share-element a[class*=" share-icon-"] {
      padding: 0 20px 0 75px /* 64px */
    }

      .share.share-element a[class^="share-icon-"]:before, .share.share-element a[class*=" share-icon-"]:before {
        position: absolute;
        font-size: 24px;
        line-height: 30px;
        height: 30px;
        width: 30px;
        top: 0;
        bottom: 0;
        left: 30px; /* 20px */
        margin: auto;
        text-align: center;
        speak: none
      }

      .share.share-element .share-icon-twitter:before {
        color: #55acee
      }

      .share.share-element .share-icon-facebook:before {
        color: #3b5998
      }

      .share.share-element .share-icon-email:before {
        color: #efe6eb
      }

    .share.share-element .share-content ul {
      position: relative;
      display: block;
      list-style-type: none;
      margin: 0;
      padding: 0
    }

      .share.share-element li {
        position: relative;
        display: block
      }

        .share.share-element li a {
          width: 100%;
          background: #2f1c27;
          color: rgba(255, 255, 255, .8);
          line-height: 60px;
          height: 60px;
          padding: 0 30px 0 75px;
          display: block;
          cursor: pointer;
          -webkit-backface-visibility: hidden;
          -webkit-touch-callout: none;
          font-size: 1.1875em;
          font-weight: normal;
          -webkit-transform: translate3d(0, 0, 0);
          transform: translate3d(0, 0, 0)
        }

          .share.share-element .share-content ul li a:hover {
            background: #27131e;
            color: #fff
          }

  .share-element li a {
    -webkit-transition: -webkit-transform 300ms;
    transition: transform 300ms;
    -webkit-transition-timing-function: cubic-bezier(.75, 0, .75, .9); /* ease rotated 180deg */
    transition-timing-function: cubic-bezier(.75, 0, .75, .9);
  }

    .share-element li:nth-child(1) a {
      -webkit-transform: translate3d(0, -60px, 0);
      transform: translate3d(0, -60px, 0);
      -webkit-transition-delay: 0ms;
      transition-delay: 0ms
    }

    .share-element li:nth-child(2) a {
      -webkit-transform: translate3d(0, -120px, 0);
      transform: translate3d(0, -120px, 0);
      -webkit-transition-delay: 100ms;
      transition-delay: 100ms
    }

    .share-element li:nth-child(3) a {
      -webkit-transform: translate3d(0, -180px, 0);
      transform: translate3d(0, -180px, 0);
      -webkit-transition-delay: 200ms;
      transition-delay: 200ms
    }

    .share-element li:nth-child(4) a {
      -webkit-transform: translate3d(0, -240px, 0);
      transform: translate3d(0, -240px, 0);
      -webkit-transition-delay: 300ms;
      transition-delay: 300ms
    }

    .share-element li:nth-child(5) a {
      -webkit-transform: translate3d(0, -300px, 0);
      transform: translate3d(0, -300px, 0);
      -webkit-transition-delay: 400ms;
      transition-delay: 400ms
    }

  .share-element.share-after-open li a {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
    -webkit-transition-timing-function: ease;
    transition-timing-function: ease
  }

    .share-element.share-after-open li:nth-last-child(1) a,
    .share-element.share-element-attached-bottom li:nth-last-child(1) a {
      -webkit-transition-delay: 0ms;
      transition-delay: 0ms
    }

    .share-element.share-after-open li:nth-last-child(2) a,
    .share-element.share-element-attached-bottom li:nth-last-child(2) a {
      -webkit-transition-delay: 100ms;
      transition-delay: 100ms
    }

    .share-element.share-after-open li:nth-last-child(3) a,
    .share-element.share-element-attached-bottom li:nth-last-child(3) a {
      -webkit-transition-delay: 200ms;
      transition-delay: 200ms
    }

    .share-element.share-after-open li:nth-last-child(4) a,
    .share-element.share-element-attached-bottom li:nth-last-child(4) a {
      -webkit-transition-delay: 300ms;
      transition-delay: 300ms
    }

    .share-element.share-after-open li:nth-last-child(5) a,
    .share-element.share-element-attached-bottom li:nth-last-child(5) a {
      -webkit-transition-delay: 400ms;
      transition-delay: 400ms
    }

    .share-element.share-element-attached-bottom li:nth-last-child(1) a {
      -webkit-transform: translate3d(0, 60px, 0);
      transform: translate3d(0, 60px, 0)
    }

    .share-element.share-element-attached-bottom li:nth-last-child(2) a {
      -webkit-transform: translate3d(0, 120px, 0);
      transform: translate3d(0, 120px, 0)
    }

    .share-element.share-element-attached-bottom li:nth-last-child(3) a {
      -webkit-transform: translate3d(0, 180px, 0);
      transform: translate3d(0, 180px, 0)
    }

    .share-element.share-element-attached-bottom li:nth-last-child(4) a {
      -webkit-transform: translate3d(0, 240px, 0);
      transform: translate3d(0, 240px, 0)
    }

    .share-element.share-element-attached-bottom li:nth-last-child(5) a {
      -webkit-transform: translate3d(0, 300px, 0);
      transform: translate3d(0, 300px, 0)
    }

  .share-element.share-element-attached-bottom.share-after-open li a {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
    -webkit-transition-timing-function: ease;
    transition-timing-function: ease
  }

    .share-element.share-element-attached-bottom.share-after-open li:nth-child(1) a {
      -webkit-transition-delay: 0ms;
      transition-delay: 0ms
    }

    .share-element.share-element-attached-bottom.share-after-open li:nth-child(2) a {
      -webkit-transition-delay: 100ms;
      transition-delay: 100ms
    }

    .share-element.share-element-attached-bottom.share-after-open li:nth-child(3) a {
      -webkit-transition-delay: 200ms;
      transition-delay: 200ms
    }
}